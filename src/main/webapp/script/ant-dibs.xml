<!-- ===================================================================== 
     - javascript taskdef : define download url for base-ide-product,base-ide-product-patch,pde-build
     ===================================================================== --> 
  <script language="javascript">
	var str = project.getProperty('GERRIT_PULL_URL');
	if( str != null){
